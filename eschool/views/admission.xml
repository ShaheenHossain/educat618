<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="openeducat_admission_view_op_admission_form" model="ir.ui.view">
            <field name="name">eschool.op.admission.inherit</field>
            <field name="model">op.admission</field>
            <field name="priority">24</field>
            <field name="inherit_id" ref="openeducat_admission.view_op_admission_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='course_id']" position="after">
                    <field name="classes_id"/>
                    <field name="section_id" />
                    <field name="roll_no" />
                </xpath>

            </field>
        </record>



    </data>
</odoo>
